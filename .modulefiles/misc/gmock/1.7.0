set version 1.7.0
proc ModulesHelp { } {
    global pkgroot version
    puts stderr "GMock & GTest $version"
    puts stderr "Google's mocking and testing frameworks"
}

module-whatis   "GMock & GTest 1.7.0 tag"

set pkgroot /opt/toolchain/gmock/1.7.0

setenv          GMOCK_ROOT       $pkgroot/
prepend-path    LD_LIBRARY_PATH  $pkgroot/lib/
prepend-path    LIBRARY_PATH     $pkgroot/lib/
